package assignments_TrainingClassess;
/*
 
 * * * *
 * * *
 * *
 * 
  
 
 */




public class PrintStarPattern_2 {

	public static void main(String[] args) {
		
			int elem = 1;
		
		for(int row=5; row>=elem; row--) {
			
			for(int col=1; col<=row; col++ ) {
				System.out.println("* ");
			}
		}
		
	}

}
